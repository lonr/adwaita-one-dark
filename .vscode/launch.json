  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Debug gtk/src/compile.ts",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/gtk/src/compile.ts",
      "outFiles": ["${workspaceFolder}/gtk/lib/**/*.js"],
      "runtimeArgs": ["--es-module-specifier-resolution=node"]
    }
  ]
}
